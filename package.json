{
  "name": "commio-native-userportal",
  "version": "1.2.1",
  "description": "Comm.io - User Portal - React Native version",
  "main": "index.js",
  "private": true,
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "android": "react-native run-android",
    "start:android": "yarn android && yarn start",
    "expo:android": "expo start --android",
    "ios": "react-native run-ios",
    "expo:ios": "expo start --ios",
    "web": "expo start --web",
    "start": "react-native start",
    "expo:start": "BROWSER=none expo start",
    "eject": "expo eject",
    "pre-commit": "lint-staged",
    "prepare": "husky install",
    "typescript": "tsc --noEmit",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "yarn lint --fix",
    "format": "prettier --write \"**/*.+(js|jsx|ts|tsx|json)\"",
    "prettify": "yarn run format && yarn run lint:fix",
    "full-check": "yarn run prettify && yarn run typescript",
    "release": "release-it",
    "test": "jest --watchAll",
    "example": "yarn --cwd example",
    "pods": "cd example && pod-install --quiet",
    "bootstrap": "yarn example && yarn && yarn pods"
  },
  "dependencies": {
    "@2600hz/commio-native-utilities": "^0.5.2",
    "@2600hz/sds-core": "^1.0.9",
    "@2600hz/sds-react-native-components": "^1.9.0",
    "@2600hz/sds-react-native-theme": "^0.2.3",
    "@2600hz/sds-themes": "^1.0.6",
    "@expo/vector-icons": "^12.0.0",
    "@react-native-async-storage/async-storage": "~1.15.0",
    "@react-native-community/masked-view": "0.1.10",
    "@react-native-picker/picker": "1.16.1",
    "@react-navigation/drawer": "^6.1.6",
    "@react-navigation/native": "^6.0.4",
    "axios": "^0.22.0",
    "expo": "^42.0.3",
    "expo-asset": "~8.3.3",
    "expo-font": "~9.2.1",
    "expo-splash-screen": "~0.11.2",
    "expo-status-bar": "~1.0.4",
    "expo-updates": "~0.8.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-native": "0.63.4",
    "react-native-gesture-handler": "~1.10.2",
    "react-native-reanimated": "~2.2.0",
    "react-native-safe-area-context": "3.2.0",
    "react-native-screens": "~3.4.0",
    "react-native-svg": "12.1.1",
    "react-native-unimodules": "~0.14.5",
    "react-native-web": "~0.13.12",
    "react-query": "^3.25.1",
    "react-test-renderer": "^17.0.2",
    "styled-components": "^5.3.1",
    "zustand": "^3.5.12"
  },
  "devDependencies": {
    "@babel/core": "~7.9.0",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@react-native-community/eslint-config": "^3.0.1",
    "@release-it/conventional-changelog": "^3.3.0",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.9.1",
    "@types/prop-types": "^15.7.4",
    "@types/react": "~16.9.35",
    "@types/react-dom": "~16.9.8",
    "@types/react-native": "~0.63.2",
    "@types/react-test-renderer": "^17.0.1",
    "@types/styled-components": "^5.1.14",
    "@types/styled-components-react-native": "^5.1.1",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "babel-loader": "^8.2.2",
    "babel-plugin-inline-import": "^3.0.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-preset-expo": "8.3.0",
    "commitlint": "^13.1.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.2",
    "jest": "^27.2.0",
    "jest-expo": "^42.0.0",
    "lint-staged": "^11.1.2",
    "metro-react-native-babel-preset": "^0.66.2",
    "node-sass": "^6.0.1",
    "pod-install": "^0.1.27",
    "prettier": "^2.4.0",
    "react-native-flipper": "^0.113.0",
    "react-native-sass-transformer": "^1.4.0",
    "react-native-svg-transformer": "^0.14.3",
    "react-query-native-devtools": "^3.0.1",
    "reactotron-react-native": "^5.0.0",
    "release-it": "^14.11.5",
    "ts-jest": "^27.0.5",
    "typescript": "~4.0.0"
  },
  "keywords": [
    "android",
    "ios",
    "mobile",
    "react",
    "react-native",
    "svg",
    "ui"
  ],
  "homepage": "https://github.com/2600hz/commio-native-userportal#readme",
